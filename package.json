{
  "name": "wasdk",
  "version": "1.0.37",
  "description": "WebAssembly SDK",
  "main": "index.js",
  "scripts": {
    "test": "node ./dist/wasdk.js sdk --test",
    "build": "tsc && tsc -p wasm-sm/tsconfig.json && tsc -p wasm-shell/tsconfig.json && node_modules/.bin/tsc -p wasm-idl/tsconfig.json",
    "postinstall": "node ./dist/wasdk.js sdk --install",
    "installdeps": "npm install --ignore-scripts",
    "clean": "rm -rf .wasdk-tmp/ bin/ lib/ node_modules/"
  },
  "bin": {
    "wasdk": "./dist/wasdk.js"
  },
  "author": "",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/yurydelendik/wasdk"
  },
  "dependencies": {
    "argparse": "^1.0.9",
    "colors": "^1.1.2",
    "decompress": "^4.0.0",
    "fs-extra": "^0.30.0",
    "graceful-fs": "^4.1.10",
    "log": "^1.4.0",
    "log-symbols": "^1.0.2",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.4",
    "tmp": "0.0.29",
    "wasdk-capstone-x86": "^1.0.6",
    "wasmparser": "^0.1.6",
    "webidl2": "^2.0.11"
  },
  "devDependencies": {
    "@types/argparse": "^1.0.30",
    "@types/colors": "^0.6.33",
    "@types/es6-promise": "0.0.31",
    "@types/fs-extra": "0.0.33",
    "@types/node": "^6.0.45",
    "@types/tmp": "0.0.31",
    "typescript": "^2.0.3"
  }
}
